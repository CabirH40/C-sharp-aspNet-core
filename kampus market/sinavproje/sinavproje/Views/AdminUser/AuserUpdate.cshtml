@model sinavproje.Models.dbuser

@{
    ViewBag.Title = "birimUpdate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Update Birim</h2>
@using (Html.BeginForm("AuserUpdate", "AdminUser", FormMethod.Post))
{
    <div class="container">
        <div class="form-group">
            <label>userName : </label>
            @Html.TextBoxFor(x => x.userName, new { @class = "form-control" })
        </div>

        <div class="form-group">
            <label>userSurname: </label>
            @Html.TextBoxFor(x => x.userSurname, new { @class = "form-control" })
        </div>

        <div class="form-group">
            <label>sehirId: </label>
            @Html.TextBoxFor(x => x.sehirId, new { @class = "form-control" })
        </div>

        <div class="form-group">
            <label>ilceId: </label>
            @Html.TextBoxFor(x => x.ilceId, new { @class = "form-control" })
        </div>

        <div class="form-group">
            <label>departmandId: </label>
            @Html.TextBoxFor(x => x.departmandId, new { @class = "form-control" })
        </div>

        <div class="form-group">
            <label>maas: </label>
            @Html.TextBoxFor(x => x.maas, new { @class = "form-control" })
        </div>

        <div class="form-group">
            <label>izin: </label>
            @Html.TextAreaFor(x => x.izin, new { @class = "form-control", @rows = 4 })
        </div>

        <div class="form-group">
            <label>password: </label>
            @Html.TextBoxFor(x => x.password, new { @class = "form-control" })
        </div>

        <div class="form-group">
            <label>role : </label>
            @Html.TextBoxFor(x => x.role, new { @class = "form-control" })
        </div>




        <div class="form-group">
            <button class="btn btn-primary">update</button>
        </div>
    </div>
}
